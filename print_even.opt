//Print sum of even numbers [0,100]

MINIMUM = 0;
MAXIMUM = 100;


iter range(min)[max] =
  min
  | lte < max?
     |> !
     $ !
;
     
     
sub iseven(val) =
  val
  | mod < 2
  | equal < 0
  | >
  
;

sub printeven(val)[even_reg] {
  val
  | iseven > even_reg;
  
  even_reg
  | ?
    val > !
    NULL_REG > !
  ;

proc main: reg {
  
  MINIMUM
  | range < MAXIMUM
  | printeven <reg
  | isnull ?
    $ !
    | printnum !
  ;
